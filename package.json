{
  "name": "imaybedepressed",
  "version": "1.0.0",
  "description": "A website containing several tests that can help you better understand how you've been feeling recently.",
  "scripts": {
    "clean": "rimraf ./dist/",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run clean &&  webpack --config webpack.prod.js",
    "dev": "npm run clean && webpack  --config webpack.dev.js && webpack serve --config webpack.dev.js",
    "watch": "webpack --watch",
    "serve": "webpack serve --config webpack.prod.js",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dolbilov/IMayBeDepressed.git"
